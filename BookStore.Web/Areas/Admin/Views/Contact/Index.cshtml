@model BookStore.Model.Models.ContactDetail
@{
    ViewBag.Title = "Trang liên hệ";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}


@section header{
    Liên hệ
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Liên hệ
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>
                                    Tên
                                </th>
                                <th>
                                    Điện thoại
                                </th>
                                <th>
                                    Email
                                </th>
                                <th>
                                    Địa chỉ
                                </th>                                
                                <th>Thao tác</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr id="row_@Model.ID">
                                <td>@Model.CompanyName</td>
                                <td>@Model.CompanyPhone</td>
                                <td>@Model.CompanyEmail</td>
                                <td>@Model.CompanyAddress</td>
                                <td>
                                    <a href="/Admin/Contact/Edit/@Model.ID">Sửa</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!-- /.table-responsive -->
            </div>
            <!-- /.panel-body -->
        </div>
    </div>
</div>

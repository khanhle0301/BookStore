    <div class="wrapper" id="wrapper">
        <header id="header" class="clearfix">
            <div class="header-top">
                <div class="container clearfix">
                    <div class="row">
                        <div id="mocua" class="col-md-5 col-sm-12">
                            @{ Html.RenderAction("Mocua", "Home");}
                        </div><!-- End .col-md-5 col-sm-12 -->

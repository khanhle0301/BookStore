                            </form>

                        </div>
                    </div><!-- End .col-md-6 -->

                    <div class="col-md-3 col-xs-12 col-sm-4" id="giaohang">
                        <a class="img-responsive" href="javascript:void(0);"><img src="/Assets/client/hstatic.net/939/1000035939/1000056344/giaohangcc31.png?v=801" alt=""></a>
                    </div><!-- End .col-md-3 -->
                </div><!-- End .row -->
            </div><!-- End .header-bottom -->
        </header><!-- /header -->
        <!-- menu -->

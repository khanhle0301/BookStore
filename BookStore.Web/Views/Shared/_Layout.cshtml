                </div>
                <div id="navbarCollapse" class="collapse navbar-collapse">
                    <ul class="nav navbar-nav clearfix">
                        @{ Html.RenderAction("Menu", "Home");}
                    </ul>
                </div>
            </div><!-- End .container -->
